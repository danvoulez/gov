{
  "$id": "https://logline.foundation/schemas/rit.ratificacao_t2.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "payload"
  ],
  "properties": {
    "payload": {
      "type": "object",
      "required": [
        "ratify_list",
        "window_days"
      ],
      "properties": {
        "ratify_list": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": [
              "id",
              "type"
            ]
          }
        },
        "window_days": {
          "type": "number",
          "minimum": 1
        }
      }
    }
  }
}