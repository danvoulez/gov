{
  "version": 2,
  "routes": [
    {
      "src": "/status/(.*)\\.json",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/status/(.*)",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|svg|woff2?)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/press/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=600"
      }
    },
    {
      "src": "/data/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=600"
      }
    }
  ]
}